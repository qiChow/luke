[
  {
    "config": {
      "name": "${get_nameValue()}",
      "variables": [],
      "request": {
        "base_url": "",
        "headers": {
          "User-Agent": "${get_userAgentValue()}"
        }
      }
    }
  },
  {
    "test": {
      "name": "${get_testNameValue()}",
      "request": {
        "params": {
          "a": "${get_testAValue()}",
          "b": "${get_testBValue()}"
        },
        "url": "${get_testUrlValue()}",
        "method": "${get_testMethodValue()}"
      },
      "validate": [
        {
          "eq": [
            "status_code",
            200
          ]
        },
        {
          "eq": [
            "headers.Content-Type",
            "application/json"
          ]
        },
        {
          "eq": [
            "content.error_code",
            0
          ]
        },
        {
          "eq": [
            "content.error_message",
            "success"
          ]
        }
      ]
    }
  }
]